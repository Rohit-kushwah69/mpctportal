<%- include('./header.ejs') %>
    <%- include('./sidebar.ejs') %>
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">Course</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
                                <li class="breadcrumb-item active">Course Display</li>
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->

            <!-- Main content -->
            <h1 class="mt-5 text-center" style="padding-top:40px">Course View</h1>
            <div class="container mt-5">
                <table class="table table-bordered text-center">
                    <thead>
                        <tr class="bg-info">
                            <th colspan="7">Display Course</th>
                        </tr>
                        <tr>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Course</th>
                            <th>Phone</th>
                            <th>Address</th>
                            <th>Status</th>
                            <th>Comment</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>
                                <%= c.name %>
                            </td>
                            <td>
                                <%= c.email %>
                            </td>
                            <td>
                                <%= c.course %>
                            </td>
                            <td>
                                <%= c.phone %>
                            </td>
                            <td>
                                <%= c.address %>
                            </td>
                            <td>
                                <%= c.status %>
                            </td>
                            <td>
                                <%= c.comment %>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->


        <%- include("./footer.ejs") %>